    },
    "devDependencies": {
        "@rimac-technology/style-guide": "^7.0.0",
        "@types/eslint": "^8.37.0",
        "@types/node": "18.16.1",
        "@types/prettier": "^2.7.2",
        "@types/react": "18.2.0",
